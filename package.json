    "url": "https://github.com/larabk/MERN-eCommerce-Course/issues"
  },
  "homepage": "https://github.com/larabk/MERN-eCommerce-Course#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
